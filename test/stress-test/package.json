{
  "name": "backend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rm -R -f ./dist && tsc && tsc-alias",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check '**.{ts,tsx,js,jsx,mjs,md,json,prisma}'",
    "prettier:fix": "prettier --write '**.{ts,tsx,js,jsx,mjs,md,json,prisma}'"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/k6": "^0.54.2",
    "@types/node": "^22.10.0",
    "eslint": "^9.13.0",
    "prettier": "^3.4.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "lint-staged": {
    "**.{ts,tsx,js,jsx,mjs,md,json,prisma}": [
      "prettier --write"
    ]
  }
}
